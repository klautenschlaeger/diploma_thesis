}

@inproceedings{behara_performance_2022,
	title = {Performance {Study} of {High}-{Efficiency} {IEEE} 802.11ax {WLAN} {Standard} {Using} {NS}-3 {Simulator}},
	doi = {10.1109/CAMAD55695.2022.9966905},
	abstract = {The future Wireless Local Area Network (WLAN) networks are expected to cater to the high-density use-case scenarios such as airports, residential apartments, outdoor stadiums, etc., where numerous Access Points (APs) are deployed to cover the given geographical area. The IEEE 802.11ax is the current generation WLAN standard designed to cater to such high-density networks. To meet the demands of high-density networks, the 802.11ax standard uses a novel frequency reuse mechanism known as Basic-Service Set (BSS)-Coloring. In this paper, we have studied the impact of the BSS-Coloring feature on the performance of the up-link throughput of 802.11ax WLAN. To this end, we have carried out Network-Simulator (NS)-3-based simulation study. Further, we have simulated a multi-BSS environment and studied the performance of throughput due to variation in the following key WLAN parameters: (i) Modulation \& Coding Scheme (MCS) Index, (ii) Channel bandwidth, (iii) Number of antennas, (iv) Access-Categories, and (v) Non-saturated Traffic conditions.},
	booktitle = {2022 {IEEE} 27th {International} {Workshop} on {Computer} {Aided} {Modeling} and {Design} of {Communication} {Links} and {Networks} ({CAMAD})},
	author = {Behara, Aparna and Venkatesh, T.G.},
	month = nov,
	year = {2022},
	note = {ISSN: 2378-4873},
	keywords = {802.11ax Wireless Local Area network (WLAN), Bandwidth, Basic Service Set (BSS) Color, IEEE 802.11ax Standard, Interference, Measurement, Modulation, Network simulator, Throughput, Wireless LAN},
	pages = {226--231},
	file = {Behara_Venkatesh_2022_Performance Study of High-Efficiency IEEE 802.pdf:/home/karlo/Zotero/storage/A8NVGZS6/Behara_Venkatesh_2022_Performance Study of High-Efficiency IEEE 802.pdf:application/pdf;IEEE Xplore Abstract Record:/home/karlo/Zotero/storage/BK4VTYM9/stamp.html:text/html},
